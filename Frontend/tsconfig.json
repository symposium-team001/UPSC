{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "baseUrl": ".",
        "strict": true,
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "jsx": "react-jsx",
        "moduleResolution": "bundler",
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}